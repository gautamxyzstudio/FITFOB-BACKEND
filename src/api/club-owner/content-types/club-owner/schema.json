{
  "kind": "collectionType",
  "collectionName": "club_owners",
  "info": {
    "singularName": "club-owner",
    "pluralName": "club-owners",
    "displayName": "Club-Owner"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ownerName": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "clubName": {
      "type": "string"
    },
    "openingTime": {
      "type": "time"
    },
    "closingTime": {
      "type": "time"
    },
    "clubPhotos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "weekday": {
      "type": "string"
    },
    "weekend": {
      "type": "string"
    },
    "facilities": {
      "type": "json"
    },
    "services": {
      "type": "json"
    },
    "latitude": {
      "type": "string"
    },
    "longitude": {
      "type": "string"
    },
    "clubAddress": {
      "type": "string"
    },
    "pincode": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
     "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
    
  }
}
